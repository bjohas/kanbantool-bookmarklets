var dest = "powerups"; /* card_types, card_template, workflow, powerups, automation_rules  */ var a=document.location + " "; if ( !a.match(/kanbantool\.com\/b\/\d+/)) { alert("You have to be on a Kanbantool.org board for this to work!"); } else { a=a.replace(/(\/b\/\d+)(\/|\-| ).*$/,"$1"); document.location=a+"/"+dest; };
